{
    "name": "cox",
    "description": "cox",
    "version": "0.0.0",
    "keywords": [
        "cox",
        "cd",
        "change dir",
        "dir",
        "project",
        "folder",
        "directory",
        "find",
        "shell",
        "command"
    ],
    "main": "index.js",
    "scripts": {
        "test": "jest",
        "test-watch": "jest --watch",
        "test-cov": "jest --coverage --runInBand --forceExit --detectOpenHandles",
        "lint": "eslint lib --cache --ignore-path .eslintignore",
        "precommit-msg": "echo 'Pre-commit checks...' && exit 0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Cox-Project/cox.git"
    },
    "author": "LvChengbin <lvchengbin59@gmail.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Cox-Project/cox/issues"
    },
    "homepage": "https://github.com/Cox-Project/cox#readme",
    "dependencies": {
        "@lvchengbin/is": "0.0.24",
        "chokidar": "^2.0.4",
        "fs-extra": "^7.0.1",
        "minimatch": "^3.0.4",
        "ynn": "0.0.14"
    },
    "devDependencies": {
        "eslint": "^5.9.0",
        "jest": "^23.6.0",
        "mock-fs": "^4.7.0",
        "nodemon": "^1.18.6",
        "pre-commit": "^1.2.2"
    },
    "engines": {
        "node": ">=8.0"
    },
    "directories": {
        "example": "example",
        "lib": "lib",
        "test": "test"
    },
    "jest": {
        "testMatch": [
            "**/test/**/*.spec.js"
        ],
        "coverageReporters": [
            "text-summary",
            "text",
            "lcov"
        ],
        "collectCoverageFrom": [
            "lib/**/*.js"
        ],
        "testEnvironment": "node"
    }
}
