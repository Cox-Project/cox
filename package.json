{
    "name": "cox",
    "description": "cox",
    "version": "0.0.0",
    "keywords": [
        "cox",
        "cd",
        "change dir",
        "dir",
        "project",
        "folder",
        "directory",
        "find",
        "shell",
        "command"
    ],
    "main": "index.js",
    "bin" : {
        "co" : "bin/co.sh",
        "cox" : "bin/cox.sh"
    },
    "scripts": {
        "test": "bats-core/bin/bats test/coecho.bats",
        "test-watch": "jest --watch",
        "test-cov": "jest --coverage --runInBand --forceExit --detectOpenHandles",
        "lint": "eslint lib --cache --ignore-path .eslintignore",
        "precommit-msg": "echo 'Pre-commit checks...' && exit 0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Cox-Project/cox.git"
    },
    "author": "LvChengbin <lvchengbin59@gmail.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Cox-Project/cox/issues"
    },
    "homepage": "https://github.com/Cox-Project/cox#readme",
    "dependencies": {},
    "devDependencies": {},
    "engines": {
        "node": ">=8.0"
    },
    "directories": {
        "example": "example",
        "lib": "lib",
        "test": "test"
    },
    "jest": {
        "testMatch": [
            "**/test/**/*.spec.js"
        ],
        "coverageReporters": [
            "text-summary",
            "text",
            "lcov"
        ],
        "collectCoverageFrom": [
            "lib/**/*.js"
        ],
        "testEnvironment": "node"
    }
}
